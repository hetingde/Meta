                "url": "https://redirector.gvt1.com/edgedl/release2/$match64#/dl.7z",
                "hash": {
                    "url": "https://scoop-apps.vercel.app/chrome",
                    "jsonpath": "$[?(@.Product == 'canary')].Release[?(@.Architecture == 'win-x64')].Hash"
