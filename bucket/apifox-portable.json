{
    "version": "2.1.11",
    "description": "Apifox = Postman + Swagger + Mock + JMeter",
    "homepage": "https://www.apifox.cn/",
    "license": {
        "identifier": "Freeware"
    },
    "notes": "We don't persist your apifox data, they are still storaged in '$env:APPDATA\\apifox'.",
    "depends": "sudo",
    "architecture": {
        "64bit": {
            "url": "http://cdn.apifox.cn/download/2.1.11/Apifox-2.1.11.exe#/dl.7z",
            "hash": "sha512:efea60263a8553876fb7d376ae8b055e8b7fe1150fe7248311a293fda1bf78916a05a6c7ea99af9e2ee690361499cd36fa80047440c60a046c49b771455a6f2f",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "@('$PLUGINSDIR', '[NSIS].nsi', 'Uninstall Apifox.exe', 'uninstallerIcon.ico') | ForEach-Object { Remove-Item \"$dir\\$_\" -Recurse -Force -ErrorAction SilentlyContinue }",
    "bin": "Apifox.exe",
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox"
        ]
    ],
    "checkver": {
        "url": "http://cdn.apifox.cn/download/latest.yml",
        "regex": "(?sm)version:[\\s]+([\\d.]+).*?http://(?<url>.*?\\.exe)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://$matchUrl#/dl.7z",
                "hash": {
                    "url": "http://cdn.apifox.cn/download/latest.yml",
                    "mode": "extract",
                    "regex": "sha512.*?$base64"
                }
            }
        }
    }
}
